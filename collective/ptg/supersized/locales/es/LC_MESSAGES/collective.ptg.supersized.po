#. Default: "Disable clicking on images. Prevents going to the image if you click on it"
#: ./__init__.py:109
msgid "label_disable_click"
msgstr "Desactivar haciendo clic en las imágenes. Evita ir a la imagen si hace clic en ella"

#. Default: "Image will never exceed browser width or height. Ignores min. dimensions"
#: ./__init__.py:86
msgid "label_fit_always"
msgstr "La imagen nunca excederá el ancho y alto del navegador. Ignorar las dimensiones mínimas"

#. Default: "Landscape images will not exceed browser width"
#: ./__init__.py:95
msgid "label_fit_landscape"
msgstr "La imagen Paisajista no excederá el ancho del navegador"

#. Default: "Portrait images will not exceed browser height"
#: ./__init__.py:91
msgid "label_fit_portrait"
msgstr "Las imágenes Portrait no sera excederá el alto del navegador"

#. Default: "Horizontally center background"
#: ./__init__.py:78
msgid "label_horizontal_center"
msgstr "Fondo de imagen centrado horizontalmente"

#. Default: "Min height allowed, in pixels"
#: ./__init__.py:74
msgid "label_min_height"
msgstr "Mínimo alto permitido, en píxeles"

#. Default: "Min width allowed, in pixels"
#: ./__init__.py:70
msgid "label_min_width"
msgstr "Mínimo ancho permitido, en píxeles"

#. Default: "Performance"
#: ./__init__.py:51
msgid "label_performance"
msgstr "Desempeño"

#. Default: "Show progress bar"
#: ./__init__.py:128
msgid "label_progress_bar"
msgstr "Mostrar barra de proceso"

#. Default: "Show slides in random order"
#: ./__init__.py:24
msgid "label_random"
msgstr "Mostrar diapositivas en orden aleatorio"

#. Default: "Hide ALL Controls"
#: ./__init__.py:132
msgid "label_show_controls"
msgstr "Ocultar TODOS los Controles"

#. Default: "Show slide names in the center of bottom tray (you will need to style 'Name' with css)"
#: ./__init__.py:114
msgid "label_slide_link"
msgstr "Muestre los nombres de las diapositivas en el centro de la bandeja inferior (tendrá que darle un estilo a 'Nombre' con css)"

#. Default: "Blank"
#: ./__init__.py:123
msgid "label_slide_links_blank"
msgstr "Blanco"

#. Default: "Name"
#: ./__init__.py:120
msgid "label_slide_links_name"
msgstr "Nombre"

#. Default: "Slideshow on"
#: ./__init__.py:16
#, fuzzy
msgid "label_slideshow"
msgstr "En diapositiva"

#. Default: "Pauses slideshow on last slide"
#: ./__init__.py:20
msgid "label_stop_loop"
msgstr "Pausar la visualización de diapositivas en la última diapositiva"

#. Default: "CSS to customize the layout"
#: ./__init__.py:136
msgid "label_supersized_css"
msgstr "Archivo CSS para personalizar el diseño"

#. Default: "Supersized"
#: ./__init__.py:145
msgid "label_supersized_display_type"
msgstr "Supersized"

#. Default: "Normal"
#: ./__init__.py:56
msgid "label_supersized_performance0"
msgstr "Normal"

#. Default: "Hybrid between speed and quality"
#: ./__init__.py:58
msgid "label_supersized_performance1"
msgstr "Híbrido entre velocidad y calidad"

#. Default: "Optimizes image quality"
#: ./__init__.py:61
msgid "label_supersized_performance2"
msgstr "Optimizar la calidad de imagen"

#. Default: "Optimizes transition speed. Only works for Firefox, IE, not Webkit"
#: ./__init__.py:64
msgid "label_supersized_performance3"
msgstr "Optimiza la velocidad de transición. Solo funciona para Firefox, IE, no para Webkit"

#. Default: "Transition"
#: ./__init__.py:28
msgid "label_supersized_transition"
msgstr "Transición"

#. Default: "None"
#: ./__init__.py:33
msgid "label_supersized_transition0"
msgstr "Ninguno"

#. Default: "Fade"
#: ./__init__.py:35
msgid "label_supersized_transition1"
msgstr "Descolorar"

#. Default: "Slide Top"
#: ./__init__.py:37
msgid "label_supersized_transition2"
msgstr "Deslizar arriba"

#. Default: "Slide Right"
#: ./__init__.py:39
msgid "label_supersized_transition3"
msgstr "Deslizar a la derecha"

#. Default: "Slide Bottom"
#: ./__init__.py:41
msgid "label_supersized_transition4"
msgstr "Deslizar abajo"

#. Default: "Slide Left"
#: ./__init__.py:43
msgid "label_supersized_transition5"
msgstr "Deslizar a la izquierda"

#. Default: "Carousel Right"
#: ./__init__.py:45
msgid "label_supersized_transition6"
msgstr "Carrusel Derecho"

#. Default: "Carousel Left"
#: ./__init__.py:47
msgid "label_supersized_transition7"
msgstr "Carrusel Izquierdo"

#. Default: "Individual thumb links for each slide in the 'bottom tray'"
#: ./__init__.py:103
msgid "label_thumb_links"
msgstr "Enlaces individual de pulgar para cada diapositiva en la 'bandeja inferior'"

#. Default: "Show next and previous thumb "
#: ./__init__.py:99
msgid "label_thumbnail_navigation"
msgstr "Mostrar pulgar en el siguiente y anterior "

#. Default: "Vertically center background"
#: ./__init__.py:82
msgid "label_vertical_center"
msgstr "Fondo de imagen centrado verticalmente"
